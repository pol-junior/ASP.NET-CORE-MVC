@model IEnumerable<WebApplication5.Models.User>


<table class="table table-dark" style="margin:0px;">
    <thead>
        <tr>
            <th scope="col">User name</th>
            <th></th>
        </tr>
    </thead>

    <tbody>
        @foreach (var user in Model)
	{
		 <tr>
             <td scope="row">@user.UserName</td>
             <td> <a asp-action="Edit" asp-route-id="@user.Id" class="btn btn-outline-warning">Edit</a></td>
         </tr>
	}
    </tbody>
</table>
